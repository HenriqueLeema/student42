{
    "C_Cpp.errorSquiggles": "disabled",
    "editor.renderWhitespace": "trailing",
    "files.associations": {
        "libftprintf.h": "c",
        "stdarg.h": "c",
        "ft_printf.h": "c",
        "stdint.h": "c",
        "string.h": "c",
        "termios.h": "c",
        "stdlib.h": "c",
        "get_next_line.h": "c",
        "unistd.h": "c",
        "fcntl.h": "c"
    },
    "[makefile]": {
        "editor.insertSpaces": false,
        "editor.tabSize": 1
    },
    "editor.tabSize": 8,
    "editor.insertSpaces": true,
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontLigatures": true,
    "editor.rulers":[
        {
            "column": 80,
            "color": "#292727b9",
            "style": "dotted"
        }
    ],
}